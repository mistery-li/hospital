	<script language="javascript" type="text/javascript">
	
		/** 漂浮公告 **/
		$(function() {
			var thisBox = $('.FloatImg');
			var defaultTop = thisBox.offset().top;
			var slide_box = $('.FloatImg .slide_box');
			var slide_min = $('.FloatImg .slide_min');

			var closed = $('.FloatImg .slide_box .close ');
			slide_min.on('click', function() {
				$(this).hide();
				slide_box.show();
			});
			closed.on('click', function() {
				slide_box.hide().hide();
				slide_min.show();
			});
			// 页面滚动的同时，悬浮框也跟着滚动
			$(window).on('scroll', function() {
				scro();
			});
			$(window).onload = scro();
			function scro() {
				var offsetTop = defaultTop + $(window).scrollTop();
				offsetTop = $("body").outerHeight() < offsetTop
						+ $(".slide_box").height() ? ($("body").outerHeight() - $(
						".slide_box").height())
						: offsetTop;
				thisBox.animate({
					top : offsetTop
				}, {
					duration : 300, //滑动速度
					queue : false
				//此动画将不进入动画队列	

				});
			}
		}); 

		var httpRequest;

		function createHttpRequest() {
			if (window.XMLHttpRequest) {
				httpRequest = new XMLHttpRequest();
			} else {
				if (window.ActiveXObject) {
					// IE
					httpRequest = new ActiveXObject("Microsoft.XMLHTTP");
				}
			}
		}

		function platformDoctIdManage() {
			var sel = document.getElementsByName("platformDoctIdSel")[0];
			var doctId = sel.options[sel.options.selectedIndex].value;
		}

		function queryOrder() {

			var platformHosId = $("#platformHosIdSel option:selected").val().split(",")[0];
			var platformDeptId = $("#platformDeptIdSel option:selected").val();
			var platformDoctorId = $("#platformDoctIdSel option:selected").val();
			var paymode = $("#platformHosIdSel option:selected").val().split(
					",")[2];
			var isSpTime = $("#platformHosIdSel option:selected").val().split(
					",")[1];
			var orgname = $("#platformHosIdSel option:selected").text();
			var reg = /^\d+$/;
			if (platformDoctorId == null) {
				platformDoctorId = "";
			}
			if (platformHosId == null || platformHosId == ""
					|| (platformHosId.indexOf("请选择") != -1)
					|| !reg.test(platformHosId)) {
				alert('请选择医院');
			} else if (platformDeptId == null || platformDeptId == ""
					|| (platformDeptId.indexOf("请选择") != -1)
					|| (platformDeptId == '-1')) {
				alert('请选择科室');
			} else if (platformDoctorId == '0' || platformDoctorId =='null'){
				formSubmit({
					platformHosId : platformHosId,
					platformDeptId : platformDeptId,
					platformDoctorId : platformDoctorId == "null" ? ""
							: platformDoctorId,
					orgname : orgname,
					tempDeptName : $("#platformDeptIdSel option:selected")
							.text(),
					paymode : paymode,
					isSpTime : isSpTime
				}, "searchOrderNumInfoAction.action");
			} else{
				formSubmit({
					isSpTime : isSpTime,
					docInfo:{deptName:$("#platformDeptIdSel option:selected").text(),
						 doctName:$("#platformDoctIdSel option:selected").text(),
						 platformDeptId:platformDeptId,
						 visitLevel:"",
						 orgcode:platformHosId,
						 platformDoctId:platformDoctorId}
				}, "forwardDocInfo.action");
			}
		}

		//ajxa查询科室
		function hospital() {

			/* var sel = document.getElementsByName("platformHosIdSel")[0];
			var selvalue = sel.options[sel.options.selectedIndex].value; */
			//platformHosIdSel
			var selvalue = $("#platformHosIdSel option:selected").val();
			var hosId = selvalue.split(",")[0];
			//如果为湘雅附二则屏蔽医生选项
			if (hosId == "444885559" || hosId == "444885014") {
				$("#platformDoctIdSel").attr("disabled", "true");
			} else {
				$("#platformDoctIdSel").removeAttr("disabled");
			}
			/* var orgname = sel.options[sel.options.selectedIndex].text; */
			var orgname = $("#platformHosIdSel option:selected").text();
			//清空科室选项
			var platformDeptIdSel = document.getElementById("platformDeptIdSel");
			$("#platformDeptIdSel").empty();
			platformDeptIdSel.options.add(new Option("--请选择科室--", ""));

			//清空医院选项
			var platformDoctIdSel = document.getElementById("platformDoctIdSel");
			platformDoctIdSel.options.length = 0;
			platformDoctIdSel.options.add(new Option("--请选择医生--", ""));
			$("#orgname").val(orgname);
			$("#hosIdInfo").val(hosId);
			$("#platformHosId").val(hosId);
			//查询医院及处理响应
			$.ajax({
				url : "findDeptment.action",
				data : "hosorgcode=" + hosId,
				dataType : "text",
				type : "POST",
				success : function(msg) {
					transHospitalInfoData(msg);
				}
			});
		}

		/**
		 * ajax方式取得医生信息后，把医生信息添加到医生选择下拉框
		 * 
		 * @returns
		 */
		function transHospitalInfoData(result) {
			var releaseObjectSelect = document.getElementById("platformDeptIdSel");
			releaseObjectSelect.options.length = 0;
			releaseObjectSelect.options.add(new Option("--请选择科室--", null));
			if (null != result && result.length > 0) {
				releaseObjectSelect.options.length = 0;
				var doctorInfosArray = result.split(";");
				for (var i = 0; i < doctorInfosArray.length; i++) {
					var doctorInfoArray = doctorInfosArray[i].split(":");
					releaseObjectSelect.options.add(new Option(
							doctorInfoArray[1], doctorInfoArray[0]));
				}
			}
			return result;
		}

		function doctManage() {
			var sel = document.getElementById("platformDeptIdSel");
			
			var DeptId = sel.options[sel.options.selectedIndex].value;
			
			//var platformHosId = document.getElementById("hosIdInfo").value;
			var platformHosId = $("#platformHosIdSel option:selected").val()
					.split(",")[0];
			$("#platformDoctIdSel").empty();
			$("#platformDoctIdSel")[0].options
					.add(new Option("--请选择医生--", null));
			if (httpRequest == null) {
				createHttpRequest();
			}

			httpRequest.onreadystatechange = transDoctInfoData;
			httpRequest.open("POST", 'findDoctInfo.action?platformHosId='
					+ platformHosId + "&hosDeptCode=" + DeptId, true);
			httpRequest.send(null);
			//查询医院及处理响应
		}

		/**
		 * ajax方式取得医生信息后，把医生信息添加到医生选择下拉框
		 * 
		 * @returns
		 */
		function transDoctInfoData() {
			if (httpRequest.readyState == 4) {

				if (!isNaN(httpRequest.status) && httpRequest.status == 200) {
					// show data here
					var result = httpRequest.responseText;
					result = result.substring(0, result.length - 1);
					var releaseObjectSelect = document
							.getElementById("platformDoctIdSel");
					if (null != result && result.length > 0) {
						releaseObjectSelect.options.length = 0;
						releaseObjectSelect.options.add(new Option("--请选择医生--",
								null));

						var doctorInfosArray = result.split(";");
						for (var i = 0; i < doctorInfosArray.length; i++) {
							var doctorInfoArray = doctorInfosArray[i]
									.split(":");
							/* if(i == 0)
							 {
							     //设置医生初始值
								// document.getElementById("platformDoctorId").value = doctorInfoArray[0];
							 }*/
							releaseObjectSelect.options.add(new Option(
									doctorInfoArray[1], doctorInfoArray[0]));
						}
					}
				} else {
					//alert('与服务器的连接发生异常！');
				}
			}
			return result;
		}

		//自动刷新一次页面的js
		var Cookie = {
			setCookie : function(name, value, option) {
				var str = name + "=" + escape(value);
				if (option) {
					if (option.expireDays) {
						var date = new Date();
						var ms = option.expireDays * 24 * 3600 * 1000;
						date.setTime(date.getTime() + ms);
						str += "; expires=" + date.toGMTString();
					}
					if (option.path)
						str += ";path=" + option.path;
					if (option.domain)
						str += ";domain=" + option.domain;
					if (option.secure)
						str += ";true";
				}
				document.cookie = str;
			},
			getCookie : function(name) {
				var cookies = document.cookie.split(";");
				for (var i = 0; i < cookies.length; i++) {
					var arr = cookies[i].split("=");
					if (arr[0] == name) {
						return unescape(arr[1]);
					}
				}
				return "";
			},
			delCookie : function(name) {
				this.setCookie(name, "", {
					expireDays : -1
				});
			}
		}
	</script>
	<div class="FloatImg" style=" width:198px;">
		
