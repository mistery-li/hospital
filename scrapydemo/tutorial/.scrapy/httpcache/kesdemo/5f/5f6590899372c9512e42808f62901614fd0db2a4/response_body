<script type="text/javaScript">

	
	var currentPage = 1;
	function switchTab(index) {
		//获取Tab个数
		var length = document.getElementById("Tab_Shi").getElementsByTagName("li").length;
		//显示Tab数
		var range = 10;
		//设置开始Tab
		currentPage += index;
		//判断并调整左移边界
		currentPage = currentPage < 1 ? 1 : currentPage;
		//设置结束Tab
		index = currentPage + range;
		//判断并调整右移边间
		currentPage = index > length ? length - range + 1 : currentPage;
		for (var i = 1; i <= length; i++) {
			var tab = document.getElementById("Tab_Shi_" + i);
			if (i >= currentPage && i <= index) {
				tab.style.display = "";
			} else {
				tab.style.display = "none";
			}
		}
	}

	function Tab_Shi_a_onclick(thiz) {
		$("#Tab_Shi>li>a[id='" + thiz + "']").attr("class", "libg");
		$("#Tab_Shi>li>a[id!='" + thiz + "']").attr("class", "");

	}
	function hospitalInfo(id) {
		$("#fl_yiyuan_nr>div[id='" + id + "']").show();
		$("#fl_yiyuan_nr>div[id!='" + id + "']").hide();
	}

	function yuyue_f1_a_onclick(thiz) {
		$(".yuyue_fl_top>li>a[class='active']").attr("class", "");
		$(thiz).attr("class", "active");

	}
</script>
