		window.onload = function() {
			/* loadHospitalHot(); */
			/* loadDoctorHot(); */
			loadRegister();
			loadNews();
			if ("1" == Cookie.getCookie("diffmaker")) {
				Cookie.delCookie("diffmaker");
			} else {
				Cookie.setCookie("diffmaker", "1", null);
				if (document.getElementById("platformHosIdSel")) {
					document.getElementById("platformHosIdSel").options[0].selected = true;
				}
			}
		}
